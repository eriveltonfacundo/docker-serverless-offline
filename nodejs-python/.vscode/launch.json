{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Node: Remote Attach",
      "type": "node",
      "request": "attach",
      "protocol": "inspector",
      "address": "localhost",
      "port": 9229,
      "localRoot": "${workspaceFolder}/src/app",
      "remoteRoot": "/usr/src/app",
      "restart": true
    },
    {
      "name": "Python: Remote Attach",
      "type": "python",
      "request": "attach",
      "port": 5678,
      "host": "localhost",
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}/src/app",
          "remoteRoot": "/usr/src/app"
        }
      ]
    }
  ]
}
